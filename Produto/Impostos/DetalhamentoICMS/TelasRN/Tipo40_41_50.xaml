<Page x:Class="Produto.Impostos.DetalhamentoICMS.TelasRN.Tipo40_41_50"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d">
    <ItemsControl Style="{StaticResource ItensResponsivos}">
        <TextBox Text="{x:Bind vICMSDeson, Mode=TwoWay}"
                 Header="Valor do ICMS desonerado"
                 InputScope="Number" />
        <TextBox Text="{x:Bind motDesICMS, Mode=TwoWay}"
                 Header="Motivo da desoneração do ICMS"
                 InputScope="Number"
                 ToolTipService.ToolTip="3 p/ agropecuária; 9 p/ outros e 12 p/ fomento e desenv. agropecuário." />
    </ItemsControl>
</Page>
