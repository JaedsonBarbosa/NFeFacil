<UserControl x:Class="Venda.Impostos.DetalhamentoICMS.TelasRN.Tipo60"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition />
			<ColumnDefinition />
			<ColumnDefinition />
		</Grid.ColumnDefinitions>
		<TextBox Text="{x:Bind vBCSTRet, Mode=TwoWay}"
				 Header="Valor da BC do ICMS ST retido"
				 InputScope="Number"
				 Margin="0,0,8,0" />
		<TextBox Grid.Column="1"
				 Text="{x:Bind pST, Mode=TwoWay}"
				 Header="Alíquota suportada pelo Consumidor Final"
				 InputScope="Number"
				 Margin="0,0,8,0" />
		<TextBox Grid.Column="2"
				 Text="{x:Bind vICMSSTRet, Mode=TwoWay}"
				 Header="Valor do ICMS ST retido"
				 InputScope="Number" />
	</Grid>
</UserControl>
