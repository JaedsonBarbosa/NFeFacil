<ContentDialog x:Class="Comum.CaixasDialogo.AdicionarReferenciaEletronica"
               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:limTexto="using:BaseGeral"
               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
               mc:Ignorable="d"
               Title="Adicionar referência"
               PrimaryButtonText="Adicionar"
               SecondaryButtonText="Cancelar">
	<TextBox TextChanged="{x:Bind limTexto:LimitadoresTextuais.PermitirNumeros}"
			 Header="Chave de acesso de uma NF-e ou NFC-e"
			 Text="{x:Bind Chave, Mode=TwoWay}" />
</ContentDialog>
